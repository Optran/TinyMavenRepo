<settings>
  <localRepository>E:/maven/mavenrepo/repository/</localRepository>
  <profiles>
    <profile>
      <id>custom</id>
      <repositories>
        <repository>
          <id>central</id>
          <url>http://localhost:9090/maven2/</url>
          <releases><enabled>true</enabled></releases>
          <snapshots><enabled>true</enabled></snapshots>
        </repository>
      </repositories>
     <pluginRepositories>
        <pluginRepository>
          <id>central</id>
          <url>http://localhost:9090/maven2/</url>
          <releases><enabled>true</enabled></releases>
          <snapshots><enabled>true</enabled></snapshots>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>custom</activeProfile>
  </activeProfiles>
</settings>
